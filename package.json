{
  "name": "blue-bug",
  "version": "1.0.0",
  "description": "My transcendent Nuxt.js project",
  "author": "Simon Suewer",
  "private": false,
  "scripts": {
    "dev": "nodemon -L server/index.js --inspect-brk=0.0.0.0 --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=development URI=http://claim-now.de node server/index.js",
    "start-dev": "node server/index.js",
    "start-master": "node server/index.js",
    "start-prod": "node server/index.js",
    "generate": "nuxt generate",
    "test": "jest --verbose",
    "lint:eslint": "eslint --ext .ts,.js,.vue .",
    "lint:style": "stylelint \"./**/*.scss\"",
    "lint:all": "npm run lint:eslint && npm run lint:style",
    "docker": "docker-compose up",
    "gen-api": "openapi-generator generate -i http://localhost/swagger.json -g typescript-axios -o generated-sources/openapi --skip-validate-spec",
    "gen-mongo": "docker exec -it  claim-now-dev-nuxt /opt/go/bin/mgodatagen -h claim-now-dev-mongo -f /app/server/config/testdata.json",
    "test-api": "lab -v server/test/user.test.js",
    "generate-server-docs": "node_modules/.bin/jsdoc -c jsdoc.json"
  },
