{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": "server/",
    "includePattern": "\\.js$",
    "excludePattern": "(node_modules/|server-docs)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "server-docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
